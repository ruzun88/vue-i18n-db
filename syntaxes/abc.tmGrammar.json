{
  "scopeName": "source.vue",
  "patterns": [{ "include": "#expression" }],
  "injectionSelector": "L:comment.line.double-slash",
  "repository": {
    "expression": {
      "patterns": [{ "include": "#i18n" }]
    },
    "letter": {
      "match": "\\$t\\([\\'\\\"]{1}[\\w.-]+[\\'\\\"]{1}\\)",
      "name": "keyword.letter"
    }
  }
}